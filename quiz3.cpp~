#include <iostream>
#include <iomanip>
#include <cmath>

using namespace std;

unsigned int seed = time(0);
int SIZE = 20;

void populate_array(int a[SIZE]);
void print_array(const int a[SIZE]);
double random (unsigned int &seed);

int main()
{
    int a[SIZE];
    populate_array(a[]);
}
void populate_array(a[])
{
    for (int i=0; i < SIZE; ++i)
	a[i] = int*random(seed);
}

